{
    "name": "asymetricdata/discord-rest-php",
    "description": "A Discord Rest client for PHP",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Asymetricdata\\DiscordRestPhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "AsymetricData"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^2.24",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.8"
    }
}
